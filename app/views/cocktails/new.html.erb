<div class="col-sm-offset-1 col-sm-11">
<h2>Add a new cocktail:<h2>

<%= simple_form_for(@cocktail) do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>


<%= link_to "Back", cocktails_path(@cocktail), {class: "btn btn-primary" } %>
</div>

